<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/vL_for_onlineReport"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:layout_marginTop="5dp"
    android:minHeight="60dp"
    android:layout_marginBottom="5dp"
    android:orientation="horizontal"
    android:weightSum="9.6">

    <TextView
        android:id="@+id/vT_aoor_lblSyncId"
        android:layout_width="0dp"
        android:textSize="@dimen/text_view_size"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Sync Id"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/vT_aoor_lblIMEI"
        android:layout_width="0dp"
        android:textSize="@dimen/text_view_size"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="IMEI #"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/vT_aoor_lblSyncActivated"
        android:layout_width="0dp"
        android:textSize="@dimen/text_view_size"
        android:layout_height="wrap_content"
        android:layout_weight="1.3"
        android:text="Sync Activated"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/vT_aoor_lblIPAdress"
        android:layout_width="0dp"
        android:textSize="@dimen/text_view_size"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="IP Address"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/vT_aoor_lblDate"
        android:layout_width="0dp"
        android:textSize="@dimen/text_view_size"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Date"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/vT_aoor_lblSyncStart"
        android:layout_width="0dp"
        android:textSize="@dimen/text_view_size"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Sync Start"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/vT_aoor_lblSyncEnd"
        android:layout_width="0dp"
        android:textSize="@dimen/text_view_size"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Sync End"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/vT_aoor_lblStatus"
        android:layout_width="0dp"
        android:textSize="@dimen/text_view_size"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Status"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/vT_aoor_lblModeofSync"
        android:layout_width="0dp"
        android:textSize="@dimen/text_view_size"
        android:layout_height="wrap_content"
        android:layout_weight="1.3"
        android:text="Mode of Sync."
        android:textColor="@color/black" />
</LinearLayout>